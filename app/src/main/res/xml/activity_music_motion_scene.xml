<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ConstraintSet android:id="@+id/start">
        <Constraint android:id="@+id/widget" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/imageView2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="24dp"
            app:layout_constraintHorizontal_bias="0.496" />
        <Constraint
            android:layout_height="51dp"
            android:layout_width="36dp"
            android:id="@+id/imageView3"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="24dp"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp" />
        <Constraint
            app:layout_constraintVertical_bias="1.0"
            android:layout_height="match_parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:id="@+id/bgroud" />
        <Constraint
            android:layout_height="51dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="40dp"
            android:id="@+id/imageView4"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            android:layout_marginTop="24dp"
            app:layout_editor_absoluteX="355dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/imageView5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/bgroud"
            android:layout_marginTop="452dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/imageView6"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="100dp"
            android:layout_marginLeft="44dp"
            android:layout_marginStart="44dp" />
        <Constraint
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/imageView7"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginEnd="44dp"
            android:layout_marginRight="44dp"
            android:layout_marginBottom="100dp" />
        <Constraint
            android:layout_height="wrap_content"
            app:layout_editor_absoluteX="136dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/imageView8"
            android:layout_marginBottom="100dp"
            app:layout_constraintStart_toEndOf="@+id/imageView6"
            android:layout_marginStart="47dp"
            android:layout_marginLeft="47dp" />
        <Constraint
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/imageView9"
            android:layout_marginBottom="100dp"
            app:layout_constraintEnd_toStartOf="@+id/imageView7"
            app:layout_constraintStart_toEndOf="@+id/imageView8" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            android:id="@+id/textView"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="28dp"
            app:layout_constraintHorizontal_bias="0.498" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint android:id="@id/widget" />
        <Constraint
            android:layout_height="107dp"
            android:layout_width="407dp"
            android:id="@+id/bgroud"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.0" />
        <Constraint
            android:layout_height="59dp"
            android:layout_width="55dp"
            android:id="@+id/imageView2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="24dp"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp" />
        <Constraint
            android:layout_height="54dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="49dp"
            android:id="@+id/imageView4"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="16dp"
            android:layout_marginRight="16dp"
            android:layout_marginBottom="24dp" />
        <Constraint
            android:layout_height="35dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="224dp"
            android:id="@+id/imageView5"
            android:layout_marginBottom="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.524" />
        <Constraint
            android:layout_height="41dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_width="40dp"
            android:id="@+id/imageView3"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="24dp"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp" />
        <Constraint
            android:layout_height="32dp"
            android:layout_width="29dp"
            android:id="@+id/imageView6"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="52dp"
            android:layout_marginLeft="96dp"
            android:layout_marginStart="96dp" />
        <Constraint
            android:layout_height="35dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="33dp"
            android:id="@+id/imageView8"
            android:layout_marginBottom="48dp"
            app:layout_constraintStart_toEndOf="@+id/imageView6"
            android:layout_marginStart="36dp"
            android:layout_marginLeft="36dp" />
        <Constraint
            android:layout_height="34dp"
            android:layout_width="32dp"
            android:id="@+id/imageView9"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="48dp"
            app:layout_editor_absoluteX="262dp" />
        <Constraint
            android:layout_height="33dp"
            android:layout_width="35dp"
            android:id="@+id/imageView7"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="48dp"
            app:layout_constraintEnd_toEndOf="@+id/bgroud"
            android:layout_marginEnd="84dp"
            android:layout_marginRight="84dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/textView"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/imageView4"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="88dp"
            app:layout_constraintHorizontal_bias="0.575" />
    </ConstraintSet>

    <Transition
        app:constraintSetEnd="@id/end"
        app:constraintSetStart="@+id/start" >
        <OnSwipe
            app:touchAnchorId="@+id/bgroud"
            app:dragDirection="dragDown" />
        <KeyFrameSet >
            <KeyAttribute
                app:motionTarget="@+id/textView"
                app:framePosition="50"
                android:alpha="0.4"
                android:rotationX="25"
                android:rotationY="-4" />
        </KeyFrameSet>
    </Transition>
</MotionScene>
